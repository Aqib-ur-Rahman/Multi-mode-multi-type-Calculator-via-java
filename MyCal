public void actionPerformed(ActionEvent e){

try{
    

if (decimal.isSelected()){
doublebtn.setEnabled(true);
bytebtn.setEnabled(true);
shortbtn.setEnabled(true);
longbtn.setEnabled(true);
perbtn.setEnabled(true);
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (addbtn.isSelected()){
            if (e.getSource()==compute){
                int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= a+b;
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}
if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (subbtn.isSelected()){
            if (e.getSource()==compute){
                int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= a-b;
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}


if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (mulbtn.isSelected()){
            if (e.getSource()==compute){
                int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= a*b;
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}


if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (divbtn.isSelected()){
            if (e.getSource()==compute){
                int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= a/b;
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}


if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (modbtn.isSelected()){
            if (e.getSource()==compute){
               int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= a%b;
                calculatedresult.setText(String.valueOf(c)); 

}
}
}
}


if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (perbtn.isSelected()){
            if (e.getSource()==compute){
               int a,b,c;
                a= Integer.parseInt(num1text.getText());
                b= Integer.parseInt(num2text.getText());
                c= (a*100)/b;
                calculatedresult.setText(String.valueOf(c+ "%")); 

}
}
}
}

if (decimal.isSelected()){
    if (bytebtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);

        if (addbtn.isSelected()){
            if (e.getSource()==compute){
                byte a,b,c;
                a= Byte.parseByte(num1text.getText());
                b= Byte.parseByte(num2text.getText());
                c= (byte) (a+b);
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}

if (decimal.isSelected()){
    if (bytebtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (subbtn.isSelected()){
            if (e.getSource()==compute){
                byte a,b,c;
                a= Byte.parseByte(num1text.getText());
                b= Byte.parseByte(num2text.getText());
                c= (byte) (a-b);
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}

if (decimal.isSelected()){
    if (bytebtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (mulbtn.isSelected()){
            if (e.getSource()==compute){
                byte a,b,c;
                a= Byte.parseByte(num1text.getText());
                b= Byte.parseByte(num2text.getText());
                c= (byte) (a*b);
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}

if (decimal.isSelected()){
    if (bytebtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
cosbtn.setEnabled(false);
tanbtn.setEnabled(false);
cotbtn.setEnabled(false);
secbtn.setEnabled(false);
cosecbtn.setEnabled(false);
sinhbtn.setEnabled(false);
coshbtn.setEnabled(false);
tanhbtn.setEnabled(false);
cothbtn.setEnabled(false);
sechbtn.setEnabled(false);
cosechbtn.setEnabled(false);
        if (divbtn.isSelected()){
            if (e.getSource()==compute){
                byte a,b,c;
                a= Byte.parseByte(num1text.getText());
                b= Byte.parseByte(num2text.getText());
                c= (byte) (a/b);
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
}


if (decimal.isSelected()){
    if (intbtn.isSelected()){
convbtn.setEnabled(false);
convbtn1.setEnabled(false);
modbtn.setEnabled(false);
expbtn.setEnabled(false);
logbtn.setEnabled(false);
sinbtn.setEnabled(false);
