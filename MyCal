            if (e.getSource()==compute){
                double a,c, b;
                a= Double.parseDouble(num1text.getText());
                b= Math.sinh(Math.toRadians(a));
             
                c= 1/b ;
                calculatedresult.setText(String.valueOf(c)); 
}
}
}
